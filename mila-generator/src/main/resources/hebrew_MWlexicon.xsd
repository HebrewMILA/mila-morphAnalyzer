      <!-- המילה כתובה באותיות עבריות -->
      <!-- המילה כתובה בתעתיק לטיני   -->
      <!-- המילה כתיבה באותיות עבריות מנוקדות -->
      <!-- כתיב -->
      <!-- נטיית שייכות חבורה -->
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="PrepositionExceptionAdd">
